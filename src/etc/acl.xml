<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="TonsOfLimes_AdminGridAI::root" title="TonsOfLimes Admin Grid AI" sortOrder="300">
                    <resource id="TonsOfLimes_AdminGridAI::grid" title="Apply State By Query for grid" sortOrder="10"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="TonsOfLimes_AdminGridAI::config" title="TonsOfLimes AdminGridAI Config"
                                      translate="title"
                                      sortOrder="10"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
